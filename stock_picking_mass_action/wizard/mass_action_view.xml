<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="view_picking_desasignar" model="ir.ui.view">
    <field name="model">picking.desasignar</field>
        <field name="arch" type="xml">
            <form string="Desasignar Series">
                <h1>Ingrese la solicitud a desasignar</h1>
                <group>
                    <field name='solicitud'/>
                </group>

                <footer>
                    <button name="confirm" string="Confirmar" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
</record>

<record id="view_form_stock_return_inherit" model="ir.ui.view">
    <field name="name">Stock Return Inherit</field>
    <field name="model">stock.return.picking</field>
    <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
    <field name="arch" type="xml">
          <xpath expr="//field[@name='location_id']" position="after">
            <field name="almacen" string="Almacen"/></xpath>
    </field>
</record>




<record id="view_clientes_reporte" model="ir.ui.view">
        <field name="model">clientes.reporte</field>
        <field name="arch" type="xml">
            <form string="Ingreso de Equipos">
                <group>
                    <field name='clientes' widget='many2many_tags' domain="[['type','!=','delivery']]"/>
                </group>

                <footer>
                    <button name="reporte" string="Reporte" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>





<record id="view_lot_retiro" model="ir.ui.view">
        <field name="model">lot.retiro</field>
        <field name="arch" type="xml">
            <form string="Ingreso de Equipos">
                <group>
                    <field name='lineas'>
                        <tree editable="bottom">
                            <field name='serie' readonly='1'/>
                            <field name="almacen" />
                            <field name="estado"  />
                            <field name="contadorMono"  />
                            <field name="contadorColor"  />
                            <field name="nivelNegro" string="Nivel Negro" />
                            <field name="nivelAmarillo" string="Nivel Amarillo" />
                            <field name="nivelCian" string="Nivel Cian"  />
                            <field name="nivelMagenta" string="Nivel Magenta"  />
                        </tree>
                    </field>
                </group>

                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>



            <record id="view_entrega_refaccion" model="ir.ui.view">
        <field name="model">entrega.action</field>
        <field name="arch" type="xml">
            <form string="Entrega de refacciones">
                <field name='tecnico'/>
                <group>
                    <field name='lines'>
                        <tree editable="bottom">
                            <field name='product_id' readonly='1'/>
                            <field name='cantidadS' readonly='1'/>
                            <field name='cantidadE'/>
                        </tree>
                    </field>
                </group>

                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    <record id="view_lot_assign_accesorios" model="ir.ui.view">
        <field name="model">lot.assign.accesorios</field>
        <field name="arch" type="xml">
            <form string="Asignación de accesorios">
                <field name="domain" invisible="1"/>
                <group>
                <field name="lineas" string="Accesorios" options='{"no_create": True}'>
                    <tree editable="bottom">
                        <field name="lot_id" string="Serie" readonly="1"/>
                        <field name="accesorios" string="Accesorios" widget="many2many_tags"/>
                    </tree>
                </field>
                </group>
                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>



