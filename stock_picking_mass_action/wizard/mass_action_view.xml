    <!-- Transfer Action -->
    <record id="action_transfer1" model="ir.actions.act_window">
        <field name="name">Transfer1</field>
        <field name="res_model">stock.picking.mass.action</field>
        <!--<field name="view_type">form</field>-->
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="context">{'check_availability': 1, 'transfer': 1,}</field>
        <field name="binding_model_id" ref="stock.model_stock_picking" />
    </record>

    <record id="view_asignacion_equipo_action_form" model="ir.ui.view">
        <field name="model">cambio.toner</field>
        <field name="arch" type="xml">
            <form string="Cambio">
                <group>
                    <notebook >
                    <field name="tonerUorden" invisible="1"/>
                        <page string="Equipos" >
                            <field name="pro_ids" attrs="{'invisible':[['tonerUorden','=',False]]}" string="Lineas">
                                <tree editable="bottom">
                                <field name="producto1" readonly="1" string="Registro Original"/>
                                <field name="tipo" invisible="1"/>
                                <field name="categoria" invisible="1"/>
                                <field name="producto2" string="Registro a Cambiar"  attrs="{'readonly':[['categoria','=',13]]}"/>
                                <field name="almacen" domain="[['x_studio_cliente','=',False]]"/>
                                <field name="existeciaAlmacen" readonly="1"/>                        
                                <field name="estado"  attrs="{'readonly':[['categoria','!=',13]]}"/>
                                <field name="serieOrigen" string="Serie" attrs="{'readonly':[['categoria','!=',13]]}"/>
                                <field name="color"  attrs="{'readonly':[['categoria','!=',13]]}"/>
                                <field name="contadorMono"  attrs="{'readonly':[['categoria','!=',13]]}"/>
                                <field name="contadorColor"  attrs="{'readonly':[['categoria','!=',13]]}"/>
                                <field name="nivelNegro" string="Nivel Negro" />
                                <field name="nivelAmarillo" string="Nivel Amarillo" attrs="{'invisible':[['color','=','B/N']]}"/>
                                <field name="nivelCian" string="Nivel Cian"  attrs="{'invisible':[['color','=','B/N']]}"/>
                                <field name="nivelMagenta" string="Nivel Magenta"  attrs="{'invisible':[['color','=','B/N']]}"/>                          
                                </tree>

                            </field>
                        </page>
                        <page string="Toner">                    
                            <field name="toner_ids" string="Toner">
                                <tree editable="bottom">
                                <field name="producto1" readonly="1" string="Registro Original"/>
                                <field name="tipo" invisible="1"/>
                                <field name="producto2" string="Registro a Cambiar" />
                                <field name="almacen" domain="[['x_studio_cliente','=',False]]"/>
                                <field name="existeciaAlmacen" readonly="1"/>                        
                                </tree>
                            </field>
                        </page>
                        <page string="Accesorios">
                            <field name="accesorios_ids" string="Accesorios">
                                <tree editable="bottom">
                                <field name="producto1" readonly="1" string="Registro Original"/>
                                <field name="tipo" invisible="1"/>
                                <field name="producto2" string="Registro a Cambiar" />
                                <field name="almacen" domain="[['x_studio_cliente','=',False]]"/>
                                <field name="existeciaAlmacen" readonly="1"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
                <footer>
                    <button name="otra" string="Apply" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    <record id="view_ingreso_almacen" model="ir.ui.view">
        <field name="model">ingreso.almacen</field>
        <field name="arch" type="xml">
            <form string="Cambio">
                <group>
                    <field name="almacen" options="{'no_create_edit': True}"/>

                        <field name="move_line" string="Productos" options="{'no_create_edit': True}">
                                <tree editable="bottom" >
                                <field name="producto" readonly="1" string="Producto"/>
                                <field name="producto2" string="Producto editable"/>
                                <field name="cantidad" />
                                </tree>
                            </field>
                </group>
                <footer>
                    <button name="confirmar" string="Ingresar Productos" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_serie_ingreso" model="ir.ui.view">
        <field name="model">serie.ingreso</field>
        <field name="arch" type="xml">
            <form string="Ingreso">
                <group>
                    <field name="almacen" options="{'no_create_edit': True}"/>

                         <field name="lineas" string="Productos" options="{'no_create_edit': True}">
                            <tree editable="bottom" options="{'no_create_edit': True}">
                                <field name="producto" readonly="1" string="Producto"/>
                                <field name="categoria" invisible="1" />
                                <field name="serie_name"  attrs="{'readonly':[['categoria','!=',13]]}"/>
                                <field name="serie"  context="{'default_product_id':producto}" invisible="1"/>
                                <field name="cantidad" />
                            </tree>
                        </field>
                        <field name="archivo"/>

                </group>
                <footer>
                    <button name="confirmar" string="Apply" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


    <record id="view_cambio_toner_action_form" model="ir.ui.view">
        <field name="model">cambio.toner</field>
        <field name="arch" type="xml">
            <form string="Cambio">
                <group>
                    <notebook >
                    <field name="tonerUorden" invisible="1"/>
                        <page attrs="{'invisible':[['tonerUorden','=',True]]}">
                            <field name="pro_ids" string="Toner">
                                <tree editable="bottom">
                                <field name="producto1" readonly="1" string="Registro Original"/>
                                <field name="tipo" invisible="1"/>
                                <field name="producto2" string="Registro a Cambiar"  />
                                <field name="almacen" domain="[['x_studio_cliente','=',False]]"/>
                                <field name="cantidad" string="Cantidad Solicitada" readonly="1"/>
                                <field name="cantidad2" string="Cantidad Enviada"/>  
                                <field name="compatibilidad" />   
                                <field name="existeciaAlmacen" />                        
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
                <footer>
                    <button name="otra" string="Apply" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

        <record id="view_guia_ticket" model="ir.ui.view">
        <field name="model">guia.ticket</field>
        <field name="arch" type="xml">
            <form string="Guia">
                <group>
                    <field name="guia">
                    </field>
                </group>
                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    <record id="view_comentario_ticket" model="ir.ui.view">
        <field name="model">comentario.ticket</field>
        <field name="arch" type="xml">
            <form string="Evidencia">
                <group>
                    <field name="comentario"/>
                    <field name="ruta" invisible="1"/>
                    <field name="evidencia" widget="many2many_binary" required="1" attrs="{'invisible': [['ruta', '=', False]]}"/>
                </group>
                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_transferencia_interna" model="ir.ui.view">
        <field name="model">transferencia.interna</field>
        <field name="arch" type="xml">
            <form string="Transferencia Interna">
                <group>
                    <field name="almacenPadre" required="1" domain="[['x_studio_cliente','=',False]]"/>
                    <field name="almacenOrigen" attrs="{'invisible':[['almacenPadre','=',False]]}"/>
                    <field name="ubicacion" invisible="1"/>
                    <field name="almacenDestino" required="1" domain="[['x_studio_cliente','=',False]]"/>
                    <field name="categoria" string="Tipo de Componentes"/>
                    <field name="archivo" string="Archivo"/>
                </group>
                <notebook>
                <page string="Productos">
                    <field name="lines" attrs="{'invisible': [['categoria', '=', False]]}"  context="{'default_ubicacion': ubicacion,'default_categoria':categoria}">
                        <tree editable="bottom">
                            <field name="producto" domain="[['categ_id','=',categoria]]"/>
                            <field name="categoria" invisible="1" />
                            <field name="modelo"/>
                            <field name="noParte"/>
                            <field name="serie" editable="1" attrs="{'invisible': [['categoria', '!=', 13]]}"/>
                            <field name="ubicacion" invisible="1"/>
                            <field name="descripcion"/>
                            <field name="disponible"/>
                            <field name="cantidad"  attrs="{'invisible': [['categoria', '=', 13]]}"/>
                        </tree>
                    </field>
                </page>
            </notebook>
                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_picking_serie" model="ir.ui.view">
        <field name="model">picking.serie</field>
        <field name="arch" type="xml">
            <form string="Serie">
                <group>
                        <field name="lines">
                            <tree editable="bottom">
                                <field name="producto" readonly="1"/>
                                <field name="estado"/>
                                <field name="serie"/>
                                <field name="contadorMono"/>
                                <field name="contadorColor"/>
                            </tree>
                        </field>
                </group>
                <footer>
                    <button name="confirmar" string="Confirmar" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>
