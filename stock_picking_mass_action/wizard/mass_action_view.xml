    <record id="view_stock_picking_mass_action_form" model="ir.ui.view">
        <field name="model">stock.picking.mass.action</field>
        <field name="arch" type="xml">
            <form string="Mass Action for the selected stock picking">
                <group>
                    <field name="check" invisible="1"/>
                    <h2 attrs="{'invisible':[['check','!=',1]]}">Se Transferiran las Ordenes Selecionadas</h2>
                    <field name="confirm" invisible="1"/>
                    <field name="check_availability" invisible="1"/>
                    <field name="transfer" invisible="1"/>
                    <field name="picking_ids" invisible="1">
                        <tree editable="bottom">
                                <field name="id" invisible="1" readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                                <field name="picking_type_id" invisible="1"/>
                                <field name="name" invisible="1" readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                                <!--<field name="x_studio_ticket" string="Ticket" readonly="1" attrs="{'invisible':[['picking_type_id','=',29314]]}"/>-->
                                <field name="origin" string="Orden" readonly="1" attrs="{'invisible':[['picking_type_id','=',29314]]}"/>
                                <!--<field name="partner_id" readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>-->
                                <field name="scheduled_date" readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                                <field name="x_studio_toneres" widget="html" string="productos" readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                                <field name="x_studio_tecnico"  domain="['|',['job_id.name','=','TÉCNICO '],['job_id.name','=','SOFTWARE']]" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                                <field name="estado"  readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                                <field name="date_done" readonly="1" attrs="{'invisible':[['picking_type_id','!=',29314]]}"/>
                        </tree>
                    </field>
                    <field name="tecnicos" attrs="{'invisible':[['check','!=',1]]}">
                        <tree editable="bottom">
                                <field name="origin" string="Orden" readonly="1" />
                                <field name="partner_id" readonly="1" />
                                <field name="scheduled_date" readonly="1" />
                                <field name="x_studio_toneres" widget="html" string="productos" readonly="1"/>
                                <field name="tecnico" readonly="0"/>
                        </tree>
                    </field>
                </group>
                <footer>
                    <button name="mass_action" string="Apply" type="object" class="oe_highlight"/>
                    <button name="vales" string="Vale de Entrega" type="object" class="oe_highlight" attrs="{'invisible':[['check','not in',[1,2]]]}"/>
                    <button name="etiquetas" string="Etiquetas" type="object" attrs="{'invisible':[['check','not in',[1,2]]]}" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

